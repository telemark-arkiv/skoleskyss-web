{
  "name": "skoleskyss-web",
  "version": "1.0.3",
  "description": "Søk skoleskyss i Telemark",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir Gåsodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "contributors": [
    {
      "name": "Jonas Enge",
      "email": "jonas.enge@gmail.com",
      "url": "https://github.com/maccyber"
    }
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.11.1"
  },
  "scripts": {
    "start": "node standalone.js",
    "dev": "NODE_ENV=development node standalone.js",
    "test": "standard &&  nsp check && tap --reporter=spec test/**/*.js",
    "coverage": "tap test/**/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/**/*.js && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "deploy": "npm test && now --npm -E production.env && now alias",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "Louie",
    "Telemark fylkeskommune",
    "Tfk",
    "Digitalisering",
    "Skoleskyss"
  ],
  "standard": {
    "ignore": [
      "public"
    ]
  },
  "louie": {
    "versionName": "Sonics",
    "versionVideoUrl": "https://www.youtube.com/watch?v=WhM5k_EGzaQ",
    "systemName": "skoleskyss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/skoleskyss-web.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/skoleskyss-web/issues"
  },
  "homepage": "https://github.com/telemark/skoleskyss-web#readme",
  "now": {
    "alias": [
      "skoleskyss.t-fk.no"
    ],
    "type": "npm",
    "engines": {
      "node": "8.11.1"
    }
  },
  "devDependencies": {
    "coveralls": "2.13.1",
    "nsp": "2.7.0",
    "standard": "10.0.3",
    "tap": "10.7.2"
  },
  "dependencies": {
    "axios": "0.16.2",
    "get-skole-aar": "1.0.0",
    "good": "7.3.0",
    "good-console": "7.1.0",
    "good-squeeze": "5.0.2",
    "handlebars": "4.0.10",
    "hapi": "16.6.3",
    "hapi-auth-cookie": "7.0.0",
    "inert": "4.2.0",
    "jsonwebtoken": "8.2.1",
    "seeiendom": "4.0.4",
    "simple-encryptor": "1.1.0",
    "tfk-saksbehandling-skoleskyss-waypoints": "1.0.11",
    "tfk-utils-fix-address-for-seeiendom": "2.0.0",
    "vision": "4.1.1",
    "wgs84-util": "3.0.0",
    "winston": "2.3.1",
    "winston-papertrail": "1.0.5",
    "yar": "8.1.2"
  }
}
